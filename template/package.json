{
  "name": "reactnativewebtemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "web": "PUBLIC_URL='/one' react-app-rewired start",
    "web:build": "PUBLIC_URL='/one' NODE_ENV=production react-app-rewired build",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint 'src/' --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npm run lint --fix"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.0.3",
    "@react-navigation/native": "^6.0.2",
    "@react-navigation/stack": "^6.0.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.2",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-safe-area-context": "^3.3.0",
    "react-native-screens": "^3.5.0",
    "react-native-web": "^0.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/runtime": "^7.15.3",
    "@types/react": "^17.0.17",
    "@types/react-native": "^0.64.13",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-react-native-web": "^0.17.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "customize-cra": "^1.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^27.0.6",
    "lint-staged": "^10.4.0",
    "metro-react-native-babel-preset": "^0.66.2",
    "prettier": "^2.1.2",
    "react-app-rewired": "^2.1.6",
    "react-scripts": "^4.0.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.0.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
